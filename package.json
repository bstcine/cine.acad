{
  "private": true,
  "scripts": {
    "start": "PORT=5001 umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@umijs/preset-react": "1.x",
    "ahooks": "^2.10.0",
    "antd": "^4.14.1",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.0-beta1",
    "classnames": "^2.2.6",
    "moment-timezone": "^0.5.33",
    "react-modal": "^3.12.1",
    "react-responsive-carousel": "^3.2.15",
    "react-router-scroll-memory": "^2.0.7",
    "tocbot": "^4.12.2",
    "umi": "^3.3.5",
    "url-parse": "^1.5.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-modal": "^3.12.0",
    "@types/url-parse": "^1.4.3",
    "@umijs/test": "^3.3.5",
    "file-loader": "^6.2.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
